{
    "trailingComma": "none",
    "semi": false,
    "jsxSingleQuote": true,
    "singleQuote": true,
    "arrowParents": "avoid",
    "tabWidth": 2,
    "importOrder": [
      "globals.css$",
      "<THIRD_PARTY_MODULES>",
      "(^@/components)|(^@/constants)|(^@/content)|(^@/hooks)|(^@/http)|(^@/services)|(^@/store)|(^@/utils)|(^@/views)",
      "(^@/models)|(.props$)",
      ".module.css$"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
  }